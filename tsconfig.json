{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": false,
    "moduleResolution": "node",
    "stripInternal": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "lib": ["es2018"]
  },
  "exclude": [
    "node_modules"
  ]
}
